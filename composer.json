{
    "name": "nvanheuverzwijn/php-config-yaml",
    "description": "A yaml configuration reader",
    "keywords": ["config", "configuration", "yaml", "yml"],
    "homepage": "http://github.com/nvanheuverzwijn/php-config-yml",
    "type": "library",
    "license": "LGPL3.0",
    "authors": [
        {
            "name": "Nicolas Vanheuverzwijn",
            "email": "nicolas.vanheu@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "nvanheuverzwijn/php-config": ">=0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {"Zwijn\\Config\\Yaml\\": "src/Config/Yaml/"}
    },
    "autoload-dev": {
        "psr-4": {"ZwijnTests\\Config\\Yaml\\": "tests/Config/Yaml/"}
    },
    "scripts": {
        "test": "phpunit"
    }
